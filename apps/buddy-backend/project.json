{
   "name": "buddy-backend",
   "$schema": "../../node_modules/nx/schemas/project-schema.json",
   "sourceRoot": "apps/buddy-backend/src",
   "projectType": "application",
   "tags": [],
   "targets": {
      "serve": {
         "executor": "@nx/js:node",
         "defaultConfiguration": "development",
         "options": {
            "buildTarget": "buddy-backend:build"
         },
         "configurations": {
            "development": {
               "buildTarget": "buddy-backend:build:development"
            },
            "production": {
               "buildTarget": "buddy-backend:build:production"
            }
         }
      },
      "test": {
         "executor": "@nx/jest:jest",
         "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
         "options": {
            "jestConfig": "apps/buddy-backend/jest.config.ts"
         }
      }
   }
}
